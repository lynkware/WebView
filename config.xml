<?xml version="1.0" encoding="UTF-8"?>
	<widget xmlns	= "http://www.w3.org/ns/widgets"
		xmlns:gap	= "http://phonegap.com/ns/1.0"
		id			= "com.vitrailaube"
		version		= "1.0.0">
	<name>Vitraiaube</name>
	<description>vitrailaube</description>
	<author href="http://http://www.lynkare.com">lynkware</author>
	<preference name="orientation" value="portrait" />
	<gap:platforms>
		<gap:platform name="android" minVersion="2.1" />
		<gap:platform name="webos" />
		<gap:platform name="symbian.wrt" />
		<gap:platform name="blackberry" project="widgets"/>
	</gap:platforms>
	<icon src="icon.png" />
	<icon src="icon57.png" gap:platform="ios" width="57" height="57" />
	<icon src="icon72.png" gap:platform="ios" width="72" height="72" />
	<gap:splash src="splash.png" />
	<gap:splash src="splash_iphone5.png" gap:platform="ios" width="640" height="1136" />
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/network"/>
</widget>